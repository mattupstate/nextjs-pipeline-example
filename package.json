{
  "name": "nextjs-pipeline-example",
  "version": "0.1.0",
  "scripts": {
    "dev": "next ./src",
    "build": "next build ./src",
    "export": "next export -o ./dist ./src",
    "start": "next start ./src",
    "test": "jest --config src/jest.config.js --watch",
    "lint": "eslint --fix .",
    "e2e": "npm run build && wdio e2e/wdio.conf.js",
    "test-ci": "jest --config src/jest.config.js",
    "lint-ci": "eslint .",
    "audit-ci": "[ $(npm audit --json | jq '.metadata.vulnerabilities.moderate + .metadata.vulnerabilities.high + .metadata.vulnerabilities.critical') = \"0\" ] || npm audit",
    "e2e-ci": "wdio e2e/wdio.ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "next": "^8.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.2",
    "@babel/register": "^7.0.0",
    "@wdio/allure-reporter": "^5.7.2",
    "@wdio/cli": "^5.7.2",
    "@wdio/jasmine-framework": "^5.7.2",
    "@wdio/junit-reporter": "^5.7.2",
    "@wdio/local-runner": "^5.7.2",
    "@wdio/selenium-standalone-service": "^5.7.2",
    "@wdio/spec-reporter": "^5.7.2",
    "@wdio/sync": "^5.7.2",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "enzyme": "3.4.3",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "jest": "^24.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "^16.8.1"
  }
}
